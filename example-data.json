  "apiEndpoints": {
    "createAdmin": {
      "method": "POST",
      "endpoint": "/api/v1.0/users/register",
      "headers": {
        "Content-Type": "application/json"
      },
      "body": {
        "name": "John Smith",
        "email": "john.smith@rvpark.com",
        "password": "admin123456",
        "confirmPassword": "admin123456",
        "phoneNumber": "+1-555-0101",
        "role": "SUPER_ADMIN",
        "preferredLocation": "Main Office"
      }
    },
    "createTenant": {
      "method": "POST",
      "endpoint": "/api/v1.0/users/register",
      "headers": {
        "Content-Type": "application/json"
      },
      "body": {
        "name": "Robert Wilson",
        "email": "robert.wilson@email.com",
        "password": "tenant123456",
        "confirmPassword": "tenant123456",
        "phoneNumber": "+1-555-0201",
        "role": "TENANT",
        "preferredLocation": "Lakeside View"
      }
    },
    "createPaymentLink": {
      "method": "POST",
      "endpoint": "/api/v1.0/stripe/create-payment-link",
      "headers": {
        "Content-Type": "application/json",
        "Authorization": "Bearer <admin-jwt-token>"
      },
      "body": {
        "tenantId": "507f1f77bcf86cd799439011",
        "currentDate": "2024-01-15T00:00:00.000Z"
      },
      "notes": "Simplified API that only requires tenantId. System automatically determines payment details from lease information and payment history. Handles first-time payments with pro-rated amounts for mid-month lease starts."
    },
    "getTenantPaymentStatus": {
      "method": "GET",
      "endpoint": "/api/v1.0/stripe/tenant-payment-status/:tenantId",
      "headers": {
        "Authorization": "Bearer <admin-jwt-token>"
      },
      "notes": "Comprehensive endpoint that checks tenant payment status, automatically creates payment links for missing payments, and handles first-time payments with proper due date calculation based on lease start date."
    },
    "inviteTenant": {
      "method": "POST",
      "endpoint": "/api/v1.0/admin/invite-tenant",
      "headers": {
        "Content-Type": "application/json",
        "Authorization": "Bearer <admin-jwt-token>"
      },
      "body": {
        "name": "New Tenant",
        "email": "new.tenant@email.com",
        "phoneNumber": "+1-555-0301",
        "preferredLocation": "Sunset Ridge",
        "propertyId": "507f1f77bcf86cd799439011",
        "spotId": "507f1f77bcf86cd799439028"
      }
    },
    "login": {
      "method": "POST",
      "endpoint": "/api/v1.0/users/login",
      "headers": {
        "Content-Type": "application/json"
      },
      "body": {
        "email": "john.smith@rvpark.com",
        "password": "admin123456"
      }
    }
  },
