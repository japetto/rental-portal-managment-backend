    "login": "curl -X POST http://localhost:5000/api/v1.0/users/login \\\n  -H \"Content-Type: application/json\" \\\n  -d '{\n    \"email\": \"john.smith@rvpark.com\",\n    \"password\": \"admin123456\"\n  }'"
