    "createTenant": "curl -X POST http://localhost:5000/api/v1.0/users/register \\\n  -H \"Content-Type: application/json\" \\\n  -d '{\n    \"name\": \"Robert Wilson\",\n    \"email\": \"robert.wilson@email.com\",\n    \"password\": \"tenant123456\",\n    \"confirmPassword\": \"tenant123456\",\n    \"phoneNumber\": \"+1-555-0201\",\n    \"role\": \"TENANT\",\n    \"preferredLocation\": \"Lakeside View\"\n  }'",
    "createPaymentLink": "curl -X POST http://localhost:5000/api/v1.0/stripe/create-payment-link \\\n  -H \"Content-Type: application/json\" \\\n  -H \"Authorization: Bearer <admin-jwt-token>\" \\\n  -d '{\n    \"tenantId\": \"507f1f77bcf86cd799439011\",\n    \"currentDate\": \"2024-01-15T00:00:00.000Z\"\n  }'",
